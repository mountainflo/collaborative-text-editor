{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        },
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-regenerator",
    "@babel/plugin-proposal-class-properties",
    ["transform-remove-console", { "exclude": [ "error", "warn", "log", "debug"] }]
  ]
}
